<div class="container text-center">
    <h2>Tu lista de celebrities</h2>
    <div class="row">
        <div class="col-4">
            <h3>name</h3>
        </div>
        <div class="col-4">
            <h3>occupation</h3>
        </div>
        <div class="col-4">
            <h3>Catch phrase</h3>
        </div>

    </div>
    <hr>
    {{#each celebrities}}
    <div class="row text-center align-items-center">
        <div class="col-4">
            <p class="mb-0">{{name}}</p>
        </div>
        <div class="col-4">
            <p class="mb-0">{{occupation}}</p>
        </div>
        <div class="col-4">
            <p class="mb-0"> {{catchPhrase}}</p>
        </div>
    </div>
    <hr>
    {{/each}}

</div>